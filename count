// 编写程序数一下 1到 100 的所有整数中出现多少次数字9。

#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int i = 0;
	int count = 0;
	for (i = 1; i <= 100; i++)
	{
		if (i % 10 == 9)
			count++;
	}
	for (i = 1; i <= 100; i++)
	{
		if (i / 10 == 9)
			count++;
	}
	printf("1-100出现9的次数为： %d \n", count);
	return 0;
}
